{
  "name": "vue-cli-plugin-import-components",
  "version": "1.0.0-0",
  "description": "Automatically import components in your Vue CLI app",
  "main": "index.js",
  "types": "dist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loonpwn/vue-cli-plugin-import-components.git"
  },
