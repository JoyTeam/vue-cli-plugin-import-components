{
  "name": "@loonpwn/vue-cli-plugin-components",
  "version": "1.0.0",
  "description": "pwa plugin for vue-cli",
  "main": "index.js",
  "types": "dist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-cli.git",
    "directory": "packages/@vue/cli-plugin-pwa"
  },
  "keywords": [
    "vue",
    "cli",
    "pwa"
  ],
  "scripts": {
