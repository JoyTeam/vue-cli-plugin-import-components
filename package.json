    "debug": "^4.3.1",
    "fast-glob": "^3.2.4",
    "minimatch": "^3.0.4"
  },
  "peerDependencies": {
    "@vue/cli-service": "^3.0.0 || ^4.0.0-0"
  },
  "devDependencies": {
